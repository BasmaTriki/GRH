	<queryString language="SQL">
		<![CDATA[SELECT iset_sf.conge.id_cong,
	iset_sf.conge.adresse_conge,
	iset_sf.conge.certaficat,
	iset_sf.conge.date_creation_conge,
	iset_sf.conge.date_debut,
